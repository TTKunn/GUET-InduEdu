# PDF解析服务环境配置文件

# ==================== 服务配置 ====================
# API服务配置
API_HOST=0.0.0.0
API_PORT=8003
API_WORKERS=1

# 跨域配置
CORS_ORIGINS=*

# ==================== PDF解析配置 ====================
# PDF解析参数
PDF_CHUNK_SIZE=1000
PDF_CHUNK_OVERLAP=200
PDF_MAX_FILE_SIZE=10

# ==================== 日志配置 ====================
# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
LOG_DIR=./logs

# ==================== 安全配置 ====================
# API密钥（可选）
# API_SECRET_KEY=your_secret_key_here
ENABLE_AUTH=false

# 请求限制
MAX_REQUESTS_PER_MINUTE=60
MAX_CONCURRENT_REQUESTS=10
